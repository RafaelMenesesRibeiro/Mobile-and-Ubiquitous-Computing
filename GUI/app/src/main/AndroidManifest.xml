<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="MobileAndUbiquitousComputing.p2photo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="MobileAndUbiquitousComputing.p2photo.NewAlbumMemberActivity"></activity>
        <activity android:name="MobileAndUbiquitousComputing.p2photo.MainMenuActivity" />
        <activity android:name="MobileAndUbiquitousComputing.p2photo.SignUpActivity" />
        <activity android:name="MobileAndUbiquitousComputing.p2photo.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="MobileAndUbiquitousComputing.p2photo.NewAlbumActivity" />
        <activity android:name="MobileAndUbiquitousComputing.p2photo.SearchUserActivity" />
        <activity android:name="MobileAndUbiquitousComputing.p2photo.ShowUserAlbumsActivity" />
        <activity android:name="MobileAndUbiquitousComputing.p2photo.AlbumViewActivity" />
    </application>

</manifest>