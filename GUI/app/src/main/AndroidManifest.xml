<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="MobileAndUbiquitousComputing.P2Photos">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="MobileAndUbiquitousComputing.P2Photos.ShowAlbumActivity"></activity>
        <activity android:name="MobileAndUbiquitousComputing.P2Photos.NewAlbumMemberActivity" />
        <activity android:name="MobileAndUbiquitousComputing.P2Photos.MainMenuActivity" />
        <activity android:name="MobileAndUbiquitousComputing.P2Photos.SignUpActivity" />
        <activity android:name="MobileAndUbiquitousComputing.P2Photos.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="MobileAndUbiquitousComputing.P2Photos.NewAlbumActivity" />
        <activity android:name="MobileAndUbiquitousComputing.P2Photos.SearchUserActivity" />
        <activity android:name="MobileAndUbiquitousComputing.P2Photos.ShowUserAlbumsActivity" />
        <activity android:name="MobileAndUbiquitousComputing.P2Photos.AlbumViewActivity" />
    </application>

</manifest>